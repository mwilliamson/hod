// name: cons
// stdout:
//   1
//   2
//   3

union List = Cons | Unit;
shape Cons {
    head: Int,
    tail: List,
}

fun cons(head: Int, tail: List) -> List {
    return Cons(head=head, tail=tail);
}

fun printInts(list: List) !io -> Unit {
    if (list is Cons) {
        print(intToString(list.head));
        printInts(list.tail);
    }
}

fun main() !io -> Unit {
    val list = cons(1, cons(2, cons(3, unit)));
    printInts(list);
}
