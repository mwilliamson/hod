// stdout:
//   1
//   2
//   3

import Options from Stdlib.Options;

union U = A | B;

fun main() !Io -> Unit {
    f!(A());
    f!(B());
}

fun f(value: U) !Io -> Unit {
    val a = cast(U, A)(value);
    if (a is Options.Some) {
        print!("is A\n");
    } else {
        print!("is not A\n");
    }

    val b = cast(U, B)(value);
    if (b is Options.Some) {
        print!("is B\n");
    } else {
        print!("is not B\n");
    }
}
