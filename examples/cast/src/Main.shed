// stdout:
//   is A
//   is not B
//   is not A
//   is B

export main;

union U = A | B;

fun main() !Io -> Unit {
    f!(A());
    f!(B());
}

fun f(value: U) !Io -> Unit {
    val a = cast(U, A)(value);
    if (a is Some) {
        print!("is A\n");
    } else {
        print!("is not A\n");
    }

    val b = cast(U, B)(value);
    if (b is Some) {
        print!("is B\n");
    } else {
        print!("is not B\n");
    }
}
