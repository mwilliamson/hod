module example;

fun fibonacci(n: Int) : Int {
    return fibonacci2(n).second;
}

shape FibonacciPair {
    first: Int,
    second: Int,
}

fun fibonacci2(n: Int) : FibonacciPair {
    if (n == 0) {
        return FibonacciPair(
            first = 1,
            second = 0
        );
    } else {
        val previous = fibonacci2(n - 1);
        return FibonacciPair(
            first = previous.second,
            second = previous.first + previous.second
        );
    }
}

