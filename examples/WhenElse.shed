// stdout:
//   a
//   b
//   other
//   other

union X = A | B | C | D;

fun main() !Io -> Unit {
    print!(toString(A()) + "\n");
    print!(toString(B()) + "\n");
    print!(toString(C()) + "\n");
    print!(toString(D()) + "\n");
}

fun toString(x : X) -> String {
    when (x) {
        is A {
            "a"
        }
        is B {
            "b"
        }
        else {
            "other"
        }
    }
}
