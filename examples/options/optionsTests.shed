import stdlib.options;
import stdlib.tests;

fun addOne(value: Int) -> Int {
    return value + 1;
}

val testCases = list(
    tests.testCase("map(f, none) is none", fun () {
        return tests.assert(options.map(addOne, options.none) is options.None);
    }),
    tests.testCase("failure", fun () {
        return tests.assert(false);
    })
);

